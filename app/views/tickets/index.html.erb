<div class="container">
  <% @tickets.each do |ticket| %>
    <div class="ticket_card">
      <p class="challenge"><%= ticket.challenge.upcase %></p>
      <p>🧑‍💻 <%= ticket.student.first_name %> <%= ticket.student.last_name %></p>
      <p>👨‍🏫 Assigned to: <span><%= ticket.teacher.first_name %> <%= ticket.teacher.last_name %></span></p>
      <p class="content"><%= ticket.content %></p>
    </div>
  <% end %>
</div>
